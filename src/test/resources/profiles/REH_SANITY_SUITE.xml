<?xml version="1.0" encoding="UTF-8"?>
<suite name="storefrontSanityTest">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<!-- <listener class-name="com.rf.core.listeners.RetryListener" /> -->
		<listener class-name="com.rf.core.listeners.TestListner" />
	</listeners>
	<test name="storefrontSanityTest">
		<classes>
			<class	name="com.rf.test.website.rehabitat.storeFront.ConsultantEnrollmentTest">
				<methods>
					<include name="testConsultantEnrollmentWithPulseAndCRP"></include>
					<include name="testConsultantEnrollmentWithoutCRPAndWithoutPulse"></include>
					<include name="testConsultantEnrollmentPersonalBusinessPortfolio_290"></include>
				</methods>
			</class>
			<class name="com.rf.test.website.rehabitat.storeFront.BillingProfileTest">
				<methods>
