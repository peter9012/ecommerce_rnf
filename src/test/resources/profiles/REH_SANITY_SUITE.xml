					<include name="testAddNewBillingAddressToNewlyCreatedBillingProfile_386"></include>					
				</methods>
			</class>
			
			<class name="com.rf.test.website.rehabitat.storeFront.shippingProfile.AddAndDeleteShippingTest">
				<methods>
					<include name="testAddShippingAddressFromMyAccount_77"></include>
					<include name="testEditAShippingAddressAndCancel_371"></include>					
