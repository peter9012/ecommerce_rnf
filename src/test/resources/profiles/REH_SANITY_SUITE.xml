				<methods>
					<include name="testAddToAutoshipCartConsultant_396"></include>									
				</methods>
			</class>
			
			<class name="com.rf.test.website.rehabitat.storeFront.PCEnrollmentTest">
				<methods>
					<include name="testPCEnrollmentWithoutSponsor"></include>	
					<include name="testPCEnrollmentWithPWSSponsor"></include>								
				</methods>
			</class>
			
			<class name="com.rf.test.website.rehabitat.storeFront.MyAccountTest">
				<methods>
					<include name="testConsultantFirstTimePulseEnrollment_380"></include>														
				</methods>
			</class>
			
			<class name="com.rf.test.website.rehabitat.storeFront.RCEnrollmentTest">
				<methods>
					<include name="testRCEnrollmentWithOrder"></include>														
				</methods>
			</class>
		</classes>
	</test>
</suite>


					