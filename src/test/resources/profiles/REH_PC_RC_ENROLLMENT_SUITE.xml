<?xml version="1.0" encoding="UTF-8"?>
<suite name="storeFrontPCAndRCEnrollmentSuite">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.rf.core.listeners.RetryListener" />
		<listener class-name="com.rf.core.listeners.TestListner" />
	</listeners>
	<test name="storeFrontPCAndRCEnrollmentTest">
		<classes>
			<class name="com.rf.test.website.rehabitat.storeFront.PCEnrollmentTest">
				<groups>
					<run>
						<exclude name="users" />
					</run>
				</groups>
			</class>
			<class name="com.rf.test.website.rehabitat.storeFront.RCEnrollmentTest">
				<groups>
					<run>
						<exclude name="users" />
					</run>
				</groups>
			</class>
			<!-- <class
				name="com.rf.test.website.rehabitat.storeFront.UserRegistrationTest">
			</class> -->
		</classes>
	</test>
</suite>


					